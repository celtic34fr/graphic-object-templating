{# template pour l'ajout de la gestion des évènements à tout objet #}
{# l'event 'dummy' permet de programmer manuellement un évènement javascript sans déclenchement de callback #}

{% for evt, callback in events %}
    {% if callback is not empty and callback != "dummy"%}
        $('#{{ objet.id }}').{{ evt }}(function (e) {
            {% if callback.class == 'javascript:' %}
            eval('{{ callback.method }}');
            {% else %}
            invokeAjax({{ objet.object }}_getData($('#{{ objet.id }}'), '{{ evt }}'), '{{ objet.id }}', '{{ evt }}', e);
            {% endif %}
        });
    {% endif %}
{% endfor %}
