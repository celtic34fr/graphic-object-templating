{% set parms = {} %}
{% if objet.controller is defined and objet.controller is not empty %}
    {% set parms = parms|merge({'controller': objet.controller}) %}
{% endif %}
{% if objet.action is defined and objet.action is not empty %}
    {% set parms = parms|merge({'action': objet.action}) %}
{% endif %}
{% if objet.params is defined and objet.params|length > 0 %}
    {% for id, valeur in objet.params %}
        {% set parms = parmms|merge({id: valeur}) %}
    {% endfor %}
{% endif %}

{% if parms|length > 0 %}
    {{ serverUrl(url(objet.route, parms)) }}
{% else %}
    {{ serverUrl(url(objet.route)) }}
{% endif %}
